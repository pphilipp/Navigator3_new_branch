<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_taxometr_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_taxometr_background"
    android:orientation="vertical"
    tools:context=".Main_taxometr" >

    <Button
        android:id="@+id/button_tarrif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/blue_line"
        android:text="ТАРИФ &quot;Тестовый&quot;"
        android:textSize="38sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/button_tarrif"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <TableLayout
                android:id="@+id/tableLayout_prices"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/button_tarrif"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:layout_marginTop="20dp" >

                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView_name_enter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Посадка"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_price_enter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_razm_enter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" грн"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView_name_kilometr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Километр"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_price_kilometr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_razm_kilometr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" грн/км"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView_name_wait"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Ожидание"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_price_wait"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_razm_wait"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" грн/мин"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:id="@+id/textView_name_downtime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Затор"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="                     "
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_price_downtime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TextView"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />

                    <TextView
                        android:id="@+id/textView_razm_downtime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" грн/мин"
                        android:textColor="#FFFFFF"
                        android:textSize="22sp" />
                </TableRow>
            </TableLayout>

            <ImageView
                android:id="@+id/ImageView01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:scaleType="fitXY"
                android:src="@drawable/gray_line" />

            <LinearLayout
                android:id="@+id/linearLayout1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/imageView2"
                android:background="@drawable/price_background_taxometr"
                android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="150dp"
                    android:gravity="left"
                    android:text="Общая стоимость:"
                    android:textColor="#f59f18"
                    android:textSize="30sp" />

                <LinearLayout
                    android:id="@+id/linearLayout_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" >

                    <TextView
                        android:id="@+id/textView_price"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="9683"
                        android:textColor="#f59f18"
                        android:textSize="120sp" />

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="."
                        android:textColor="#f59f18"
                        android:textSize="70sp" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:text="грн"
                            android:textColor="#f59f18"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/textview_price_after_dot"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:gravity="left|top"
                            android:text="15"
                            android:textColor="#f59f18"
                            android:textSize="60dp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" android:orientation="horizontal" android:layout_marginTop="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imageView_face"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/ic_launcher" />

                <TextView
                    android:id="@+id/textView_surname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Фамилия"
                    android:textColor="#f59f18"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/textView_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Имя"
                    android:textColor="#FFFFFF"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/textView_patronymic"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Отчество"
                    android:textColor="#FFFFFF"
                    android:textSize="25sp" />

                <Button
                    android:id="@+id/button_cancel_taxometr"
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:background="@drawable/button_blue"
                    android:text="ОТМЕНА"
                    android:textSize="38sp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" android:paddingRight="10dp">

                <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/img_do_my_order" android:background="@drawable/main_textview_boards1"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="2dp"
                        android:text="время в пути" android:layout_centerHorizontal="true"/>
                    <TextView
                        android:id="@+id/textView_time_in_road"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="TextView"
                        android:textSize="25sp" android:layout_centerInParent="true" android:paddingTop="10dp"/>
                </RelativeLayout>
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                android:id="@+id/relativeLayout" android:layout_marginTop="5dp">
                    <ImageView android:layout_width="match_parent" android:layout_height="wrap_content"
                               android:id="@+id/imageView2" android:background="@drawable/main_textview_boards1"/>
                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                              android:layout_gravity="center_horizontal" android:layout_marginTop="2dp"
                              android:text="расстояние" android:layout_centerHorizontal="true"/>
                    <TextView android:id="@+id/textView_distance" android:layout_width="wrap_content"
                              android:layout_height="wrap_content" android:layout_gravity="center"
                              android:text="TextView" android:textSize="25sp" android:layout_centerInParent="true"
                              android:paddingTop="10dp"/>
                </RelativeLayout>
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                                android:id="@+id/relativeLayout2" android:layout_marginTop="5dp">
                    <ImageView android:layout_width="match_parent" android:layout_height="wrap_content"
                               android:id="@+id/imageView3" android:background="@drawable/main_textview_boards1"/>
                    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                              android:layout_gravity="center_horizontal" android:layout_marginTop="2dp"
                              android:text="скорость" android:layout_centerHorizontal="true"/>
                    <TextView android:id="@+id/textView_speed" android:layout_width="wrap_content"
                              android:layout_height="wrap_content" android:layout_gravity="center"
                              android:text="TextView" android:textSize="25sp" android:layout_centerInParent="true"
                              android:paddingTop="10dp"/>
                </RelativeLayout>

                <FrameLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">
                    <Button
                        android:id="@+id/button_start_taxometr"
                        android:layout_width="match_parent"
                        android:layout_height="110dp"
                        android:background="@drawable/button_black"
                        android:text="СТАРТ"
                        android:textSize="38sp" android:enabled="false" android:layout_gravity="bottom"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
    </LinearLayout>
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/arrow" />

</RelativeLayout>